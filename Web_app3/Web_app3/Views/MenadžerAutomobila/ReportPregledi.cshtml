@model IEnumerable<AutoServis.Models.VoziloProdaja>

<h2>Vozila prema broju pregleda</h2>


<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
        <tr>
            <th>
                Model
            </th>
            <th>
                Marka

            </th>
            <th>
               Datum proizvodnje

            </th>

            <th>
                Broj pregleda
            </th>




        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tbody>

            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Model.Naziv)

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Model.marka.Nazvi)

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DatumProizvodnje)

                </td>


                <td>
                    @Html.DisplayFor(modelItem => item.brojPregleda)
                </td>


            </tr>

        </tbody>
    }
</table>



